<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="articlesRowModel" type="com.phngsapplication.app.modules.articles.data.model.ArticlesRowModel"/>
    </data>
    <FrameLayout android:id="@+id/frameStackdavidaustinw"
        android:layout_height="wrap_content" android:layout_width="match_parent">
        <LinearLayout android:id="@+id/linearColumndavidaustinw"
            android:layout_gravity="bottom|start"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A702cornerRadius">
            <TextView android:id="@+id/txtDavidAustinW"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:text="@{articlesRowModel.txtDavidAustinW}"
                style="@style/txtAndadaProromanbold15" tools:text="@string/msg_david_austin_w"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowellipsethirtyone"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_17pxv"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageEllipseThirtyOne"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_ellipse31}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse31"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnshivanivora"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_9pxh"
                    android:layout_weight="0.7"
                    android:layout_width="0dp" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtShivaniVora"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{articlesRowModel.txtShivaniVora}"
                        style="@style/txtAndadaProromanmedium11" tools:text="@string/lbl_shivani_vora"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txt20190101"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{articlesRowModel.txt20190101}"
                        style="@style/txtAndadaProromanmedium9" tools:text="@string/lbl_2019_01_01"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageUser"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_9pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="@dimen/_56pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
            </LinearLayout>
        </LinearLayout>
        <ImageView android:id="@+id/imageRectangleTwentyTwo"
            android:layout_gravity="top|start"
            android:layout_height="@dimen/_145pxh"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_width="@dimen/_331pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_13pxh}"
            imageRes="@{@drawable/img_rectangle22}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle22"/>
    </FrameLayout>
</layout>
