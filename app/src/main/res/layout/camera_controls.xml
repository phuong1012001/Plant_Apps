<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/camera_controls_container"
        android:layoutDirection="ltr"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:id="@+id/linearColumnairplane"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_40pxv"
            android:layout_marginEnd="@dimen/_40pxv"
            android:layout_weight="0.99"
            android:gravity="start|center_horizontal"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_4pxh"
                android:layout_marginEnd="@dimen/_4pxh"
                android:gravity="center_horizontal"
                app:contentInsetStart="0dp">

                <LinearLayout
                    android:id="@+id/linearRow"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/linearRowairplane"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.45"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageAirplane"
                            android:layout_width="@dimen/_10pxh"
                            android:layout_height="@dimen/_19pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_airplane"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_airplane" />

                        <TextView
                            android:id="@+id/txtAUTO"
                            style="@style/txtAndadaProromanbold13"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_4pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_weight="0.79"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="@string/lbl_auto"
                            tools:ignore="TextSizeCheck" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearRowgrid"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_138pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.6"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageGrid"
                            android:layout_width="@dimen/_17pxh"
                            android:layout_height="@dimen/_17pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_grid"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_grid" />

                        <FrameLayout
                            android:id="@+id/frameStackellipse"
                            android:layout_width="@dimen/_19pxh"
                            android:layout_height="@dimen/_18pxv"
                            android:layout_marginStart="@dimen/_22pxh"
                            android:layout_marginEnd="@dimen/_22pxh">

                            <View
                                android:id="@+id/viewEllipse"
                                android:layout_width="@dimen/_11pxh"
                                android:layout_height="@dimen/_11pxh"
                                android:layout_gravity="top|center"
                                android:layout_marginStart="@dimen/_4pxh"
                                android:layout_marginEnd="@dimen/_4pxh"
                                android:layout_marginBottom="@dimen/_10pxv"
                                android:background="@drawable/rectangle_bg_white_a700_4c_radius_5_5" />

                            <View
                                android:id="@+id/viewEllipseOne"
                                android:layout_width="@dimen/_11pxh"
                                android:layout_height="@dimen/_11pxh"
                                android:layout_gravity="bottom|start"
                                android:layout_marginTop="@dimen/_10pxv"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:background="@drawable/rectangle_bg_white_a700_cc_radius_5_5" />

                            <View
                                android:id="@+id/viewEllipseTwo"
                                android:layout_width="@dimen/_11pxh"
                                android:layout_height="@dimen/_11pxh"
                                android:layout_gravity="bottom|end"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_10pxv"
                                android:background="@drawable/rectangle_bg_white_a700_99_radius_5_5" />
                        </FrameLayout>

                        <LinearLayout
                            android:id="@+id/linearColumnellipsetwoOne"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="end"
                            android:orientation="vertical"
                            app:layout_constraintEnd_toStartOf="@+id/linearColumnairplane">

                            <View
                                android:id="@+id/viewEllipseTwoOne"
                                android:layout_width="@dimen/_4pxh"
                                android:layout_height="@dimen/_4pxh"
                                android:background="@drawable/rectangle_bg_white_a702_radius_2" />

                            <View
                                android:id="@+id/viewEllipseTwentyOne"
                                android:layout_width="@dimen/_4pxh"
                                android:layout_height="@dimen/_4pxh"
                                android:layout_marginTop="@dimen/_3pxv"
                                android:background="@drawable/rectangle_bg_white_a702_radius_2" />

                            <View
                                android:id="@+id/viewEllipseTwentyTwo"
                                android:layout_width="@dimen/_4pxh"
                                android:layout_height="@dimen/_4pxh"
                                android:layout_marginTop="@dimen/_3pxv"
                                android:background="@drawable/rectangle_bg_white_a702_radius_2" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>

            <LinearLayout
                android:id="@+id/linearRowrectangle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_620pxv"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageRectangle"
                    cornerRadius="@{@dimen/_3pxh}"
                    android:layout_width="@dimen/_40pxh"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_gravity="center|right"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle"
                    tools:ignore="ContentDescription" />

                <ImageButton
                    android:id="@+id/camera_capture_button"
                    android:layout_width="@dimen/_63pxh"
                    android:layout_height="@dimen/_63pxh"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginStart="@dimen/_80pxh"
                    android:background="@drawable/rectangle_border_white_a702_radius_31_5" />

                <ImageView
                    android:id="@+id/camera_switch_button"
                    android:layout_width="@dimen/_26pxh"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_gravity="center|right"
                    android:layout_marginStart="@dimen/_80pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_refresh"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_refresh" />
            </LinearLayout>

            <View
                android:id="@+id/viewEllipseTwo1"
                android:layout_width="@dimen/_3pxh"
                android:layout_height="@dimen/_3pxh"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="@dimen/_8pxv"
                android:background="@drawable/rectangle_bg_white_a702_radius_1_5" />

            <TextView
                android:id="@+id/txtPHOTO"
                style="@style/txtAndadaProromanbold13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@string/lbl_photo"
                tools:ignore="TextSizeCheck" />
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>