<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="onboarding3ReadManyArticlesAboutPlantsVM" type="com.phngsapplication.app.modules.onboarding3readmanyarticlesaboutplants.data.viewmodel.Onboarding3ReadManyArticlesAboutPlantsVM"/>
    </data>
    <FrameLayout android:id="@+id/frameOnboarding3"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnellipsethirtyone"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_22pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_width="match_parent" android:orientation="vertical">

            <FrameLayout
                android:id="@+id/frameStackreply"
                android:layout_width="@dimen/_300pxh"
                android:layout_height="@dimen/_300pxv"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_marginEnd="@dimen/_6pxh">

                <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/s1" />
            </FrameLayout>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtReadManyArtic"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_13pxh"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_66pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{onboarding3ReadManyArticlesAboutPlantsVM.onboarding3ReadManyArticlesAboutPlantsModel.txtReadManyArtic}"
                style="@style/txtAndadaProromanbold20" tools:text="@string/msg_read_many_artic"/>

            <TextView
                android:id="@+id/txtDescription"
                style="@style/txtAndadaProromanregular15"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_marginEnd="@dimen/_2pxh"
                android:gravity="center_horizontal"
                android:text="@{onboarding3ReadManyArticlesAboutPlantsVM.onboarding3ReadManyArticlesAboutPlantsModel.txtDescription}"
                tools:ignore="TextSizeCheck"
                tools:text="@string/msg_let_s_learn_mor" />

            <LinearLayout
                android:id="@+id/linearRowellipsetwentythree"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_140pxh"
                android:layout_marginTop="@dimen/_50pxv"
                android:layout_marginEnd="@dimen/_140pxh"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                tools:layout_gravity="center">

                <View
                    android:id="@+id/viewEllipseTwentyThree"
                    android:layout_width="@dimen/_7pxh"
                    android:layout_height="@dimen/_7pxh"
                    android:background="@drawable/rectangle_bg_gray_301_radius_3_5" />

                <View
                    android:id="@+id/viewEllipseTwentyFour"
                    android:layout_width="@dimen/_7pxh"
                    android:layout_height="@dimen/_7pxh"
                    android:layout_marginStart="@dimen/_9pxh"
                    android:background="@drawable/rectangle_bg_gray_301_radius_3_5" />

                <View
                    android:id="@+id/viewEllipseTwentyFive"
                    android:layout_width="@dimen/_7pxh"
                    android:layout_height="@dimen/_7pxh"
                    android:layout_marginStart="@dimen/_9pxh"
                    android:background="@drawable/rectangle_bg_teal_a402_radius_3_5" />
            </LinearLayout>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btnSignIn"
                style="@style/btnSolidRounded_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="@dimen/_54pxv"
                android:ellipsize="end"
                android:gravity="center"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_16pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingBottom="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@string/lbl_sign_in"
                android:textAllCaps="false"
                tools:ignore="TextSizeCheck"
                tools:text="@string/lbl_sign_in" />
        </LinearLayout>
    </FrameLayout>
</layout>
