<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <data>

    </data>
<!--    <variable name="detailArticlesVM" type="com.phngsapplication.app.modules.detailarticles.data.viewmodel.DetailArticlesVM"/>-->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/linearColumnThree"
        style="@style/groupStylewhite_A700"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="start"
        android:orientation="vertical">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout2"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_297pxv"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            style="@style/groupStylewhite_A700">

            <FrameLayout
                android:id="@+id/frameStackvegetables"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_297pxv"
                android:gravity="start"
                tools:ignore="MissingConstraints">

                <FrameLayout
                    android:id="@+id/frameStackrectangle"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_249pxv"
                    android:layout_gravity="top|start"
                    android:layout_marginBottom="@dimen/_10pxv">

                    <ImageView
                        android:id="@+id/imageArticle"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_249pxv"
                        android:layout_gravity="start|center_horizontal"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_rectangle_249x375" />

                    <ImageView
                        android:id="@+id/imageBack"
                        android:layout_width="@dimen/_11pxh"
                        android:layout_height="@dimen/_19pxv"
                        android:layout_gravity="top|start"
                        android:layout_marginStart="@dimen/_23pxh"
                        android:layout_marginTop="@dimen/_30pxv"
                        android:layout_marginEnd="@dimen/_23pxh"
                        android:layout_marginBottom="@dimen/_30pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_arrowleft" />
                </FrameLayout>

                <LinearLayout
                    android:id="@+id/linearRowvegetables"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|start"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_23pxh"
                    android:orientation="horizontal">

                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/btnVegetables"
                        style="@style/btnSolidRounded_2"
                        android:layout_width="@dimen/_106pxh"
                        android:layout_height="@dimen/_23pxv"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:paddingStart="@dimen/_9pxh"
                        android:paddingTop="@dimen/_4pxv"
                        android:paddingEnd="@dimen/_15pxh"
                        android:paddingBottom="@dimen/_4pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_vegetables"
                        android:textAllCaps="false"
                        tools:ignore="TextSizeCheck,TouchTargetSizeCheck"
                        tools:text="@string/lbl_vegetables" />

                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/btnGarden"
                        style="@style/btnSolidRounded_2"
                        android:layout_width="@dimen/_87pxh"
                        android:layout_height="@dimen/_23pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:paddingStart="@dimen/_13pxh"
                        android:paddingTop="@dimen/_4pxv"
                        android:paddingEnd="@dimen/_20pxh"
                        android:paddingBottom="@dimen/_4pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_garden"
                        android:textAllCaps="false"
                        tools:ignore="TextSizeCheck,TouchTargetSizeCheck"
                        tools:text="@string/lbl_garden" />
                </LinearLayout>

                <ImageButton
                    android:id="@+id/btnLike"
                    android:layout_width="@dimen/_57pxh"
                    android:layout_height="@dimen/_57pxh"
                    android:layout_gravity="bottom|end"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_marginEnd="@dimen/_23pxh"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:background="@drawable/rectangle_bg_red_a200_radius_28_5"
                    android:paddingHorizontal="@dimen/_15pxh"
                    android:paddingVertical="@dimen/_15pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_group50"
                    tools:ignore="ContentDescription,SpeakableTextPresentCheck" />
            </FrameLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/_305pxv"
            style="@style/groupStylewhite_A700"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <LinearLayout
                    android:id="@+id/linearDetailArticles"
                    style="@style/groupStylewhite_A700"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/txtTitleArticle"
                        style="@style/txtAndadaProromanbold20"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_22pxh"
                        android:layout_marginTop="@dimen/_19pxv"
                        android:layout_marginEnd="@dimen/_22pxh"
                        android:gravity="center_horizontal"
                        android:text="@string/msg_even_on_urban_e2"
                        tools:text="@string/msg_even_on_urban_e2" />

                    <LinearLayout
                        android:id="@+id/linearRowellipsethree"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_22pxh"
                        android:layout_marginTop="@dimen/_15pxv"
                        android:layout_marginEnd="@dimen/_22pxh"
                        android:gravity="start"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageAuthor"
                            circular="@{true}"
                            android:layout_width="@dimen/_37pxh"
                            android:layout_height="@dimen/_37pxh"
                            android:scaleType="centerCrop"
                            android:src="@drawable/img_ellipse3_37x37" />

                        <LinearLayout
                            android:id="@+id/linearColumnshyllamonic"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.64"
                            android:gravity="start"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/txtAuthor"
                                style="@style/txtAndadaProromanbold14_4"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:ellipsize="end"
                                android:singleLine="true"
                                android:text="@string/lbl_shylla_monic"
                                tools:ignore="TextSizeCheck"
                                tools:text="@string/lbl_shylla_monic" />

                            <TextView
                                android:id="@+id/txtDate"
                                style="@style/txtAndadaProromanmedium12_1"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_4pxv"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:ellipsize="end"
                                android:singleLine="true"
                                android:text="@string/lbl_2019_01_01"
                                tools:ignore="TextSizeCheck"
                                tools:text="@string/lbl_2019_01_01" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/btnFollow"
                            style="@style/groupStyleteal_A402cornerRadius"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_weight="0.24"
                            android:gravity="center_horizontal"
                            android:orientation="vertical">

                            <LinearLayout
                                android:id="@+id/linearRowplus"
                                style="@style/groupStylecornerRadius_1"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginStart="@dimen/_14pxh"
                                android:layout_marginTop="@dimen/_8pxv"
                                android:layout_marginEnd="@dimen/_13pxh"
                                android:layout_marginBottom="@dimen/_9pxv"
                                android:gravity="center_vertical"
                                android:orientation="horizontal">

                                <ImageView
                                    android:layout_width="@dimen/_7pxh"
                                    android:layout_height="@dimen/_7pxh"
                                    android:layout_marginTop="@dimen/_3pxv"
                                    android:layout_marginBottom="@dimen/_1pxv"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_plus"
                                    tools:ignore="ContentDescription,DuplicateIds"
                                    tools:src="@drawable/img_plus" />

                                <TextView
                                    android:id="@+id/txtFollow"
                                    style="@style/txtAndadaProromanextrabold10"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_6pxh"
                                    android:layout_weight="0.82"
                                    android:ellipsize="end"
                                    android:singleLine="true"
                                    android:text="@string/lbl_follow"
                                    tools:ignore="TextSizeCheck"
                                    tools:text="@string/lbl_follow" />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>

                    <TextView
                        android:id="@+id/txtDescription"
                        style="@style/txtAndadaProromanregular14_1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_22pxh"
                        android:layout_marginTop="@dimen/_24pxv"
                        android:layout_marginEnd="@dimen/_22pxh"
                        android:gravity="center_horizontal"
                        android:letterSpacing="0.01"
                        android:text="@string/msg_public_parks_as"
                        tools:ignore="TextSizeCheck"
                        tools:text="@string/msg_public_parks_as" />
                </LinearLayout>
            </ScrollView>
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/frameBottombar"
            style="@style/groupStyle_1"
            android:layout_width="@dimen/_370pxh"
            android:layout_height="@dimen/_115pxv"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent">

            <FrameLayout
                android:id="@+id/frameStackunion"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_115pxv"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent">

                <ImageView
                    android:id="@+id/imageUnion"
                    imageRes="@{@drawable/img_union_1}"
                    android:layout_width="@dimen/_375pxh"
                    android:layout_height="@dimen/_115pxv"
                    android:layout_gravity="center"
                    android:scaleType="fitXY"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_union_1" />

                <LinearLayout
                    android:id="@+id/linearRow"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/btnHome"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_50pxv"
                        android:layout_weight="0.25"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/imageHomeOne"
                            android:layout_width="@dimen/_25pxh"
                            android:layout_height="@dimen/_23pxh"
                            android:layout_gravity="center"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_home"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_home" />

                        <TextView
                            android:id="@+id/txtHOMETwo"
                            style="@style/txtAndadaProromanbold10_1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_9pxv"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:singleLine="true"
                            android:text="@string/lbl_home"
                            tools:ignore="TextSizeCheck"
                            tools:text="@string/lbl_home" />
                    </LinearLayout>

                    <FrameLayout
                        android:id="@+id/btnAdd"
                        style="@style/groupStyleblue_400cornerRadius"
                        android:layout_width="@dimen/_58pxh"
                        android:layout_height="@dimen/_58pxv"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_marginBottom="@dimen/_30pxv">

                        <ImageView
                            android:id="@+id/imagePlus"
                            android:layout_width="@dimen/_21pxh"
                            android:layout_height="@dimen/_21pxh"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/_18pxv"
                            android:layout_marginBottom="@dimen/_17pxv"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_plus"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_plus" />
                    </FrameLayout>

                    <LinearLayout
                        android:id="@+id/btnProfile"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_50pxv"
                        android:layout_weight="0.25"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/imageUser"
                            android:layout_width="@dimen/_19pxh"
                            android:layout_height="@dimen/_25pxh"
                            android:layout_gravity="center"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_user_bluegray_100"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_user_bluegray_100" />

                        <TextView
                            android:id="@+id/txtPROFILE"
                            style="@style/txtAndadaProromanbold10_2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_2pxh"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:singleLine="true"
                            android:text="@string/lbl_profile"
                            tools:ignore="TextSizeCheck"
                            tools:text="@string/lbl_profile" />
                    </LinearLayout>

                </LinearLayout>

            </FrameLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
