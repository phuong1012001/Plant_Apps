<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="detailArticlesVM" type="com.phngsapplication.app.modules.detailarticles.data.viewmodel.DetailArticlesVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnrectangle"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearDetailArticles"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameStackvegetables"
                android:layout_height="@dimen/_297pxv" android:layout_width="match_parent">
                <FrameLayout android:id="@+id/frameStackrectangle"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_249pxv"
                    android:layout_marginBottom="@dimen/_10pxv" android:layout_width="match_parent">
                    <ImageView android:id="@+id/imageRectangle"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_249pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_rectangle_249x375}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_249x375"/>
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_19pxh"
                        android:layout_marginBottom="@dimen/_30pxv"
                        android:layout_marginEnd="@dimen/_23pxh"
                        android:layout_marginStart="@dimen/_23pxh"
                        android:layout_marginTop="@dimen/_30pxv"
                        android:layout_width="@dimen/_11pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                </FrameLayout>
                <LinearLayout android:id="@+id/linearRowvegetables"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_23pxh"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnVegetables"
                        android:layout_height="@dimen/_23pxv"
                        android:layout_width="@dimen/_106pxh"
                        android:paddingBottom="@dimen/_4pxv"
                        android:paddingEnd="@dimen/_15pxh"
                        android:paddingStart="@dimen/_9pxh"
                        android:paddingTop="@dimen/_4pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_vegetables"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded_2" tools:text="@string/lbl_vegetables"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnGarden"
                        android:layout_height="@dimen/_23pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_width="@dimen/_87pxh"
                        android:paddingBottom="@dimen/_4pxv"
                        android:paddingEnd="@dimen/_20pxh"
                        android:paddingStart="@dimen/_13pxh"
                        android:paddingTop="@dimen/_4pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_garden"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded_2" tools:text="@string/lbl_garden"/>
                </LinearLayout>
                <ImageButton
                    android:background="@drawable/rectangle_bg_red_a200_radius_28_5"
                    android:id="@+id/btnGroupFifty"
                    android:layout_gravity="bottom|end"
                    android:layout_height="@dimen/_57pxh"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:layout_marginEnd="@dimen/_23pxh"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_width="@dimen/_57pxh"
                    android:paddingHorizontal="@dimen/_15pxh"
                    android:paddingVertical="@dimen/_15pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_group50" tools:ignore="ContentDescription"/>
            </FrameLayout>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtEvenonUrbanE"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="match_parent"
                android:text="@{detailArticlesVM.detailArticlesModel.txtEvenonUrbanE}"
                style="@style/txtAndadaProromanbold20" tools:text="@string/msg_even_on_urban_e2"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowellipsethree"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageEllipseThree"
                    android:layout_height="@dimen/_37pxh"
                    android:layout_width="@dimen/_37pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_ellipse3_37x37}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse3_37x37"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnshyllamonic"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.64"
                    android:layout_width="0dp" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtShyllaMonic"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{detailArticlesVM.detailArticlesModel.txtShyllaMonic}"
                        style="@style/txtAndadaProromanbold14_4" tools:text="@string/lbl_shylla_monic"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDate"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{detailArticlesVM.detailArticlesModel.txt20190101}"
                        style="@style/txtAndadaProromanmedium12_1" tools:text="@string/lbl_2019_01_01"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnplus"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_weight="0.24"
                    android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStyleteal_A402cornerRadius">
                    <LinearLayout android:gravity="center_vertical"
                        android:id="@+id/linearRowplus"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginEnd="@dimen/_13pxh"
                        android:layout_marginStart="@dimen/_14pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStylecornerRadius_1">
                        <ImageView android:id="@+id/imagePlus"
                            android:layout_height="@dimen/_7pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_width="@dimen/_7pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_plus"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_plus"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtFollow"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_6pxh"
                            android:layout_weight="0.82"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{detailArticlesVM.detailArticlesModel.txtFollow}"
                            style="@style/txtAndadaProromanextrabold10" tools:text="@string/lbl_follow"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtDescription"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent"
                android:letterSpacing="0.01"
                android:text="@{detailArticlesVM.detailArticlesModel.txtDescription}"
                style="@style/txtAndadaProromanregular14_1" tools:text="@string/msg_public_parks_as"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStyle_1">
            <FrameLayout android:id="@+id/frameStackrectanglethree"
                android:layout_height="@dimen/_115pxv" android:layout_width="match_parent">
                <View android:background="@color/white_A702"
                    android:id="@+id/viewRectangleThree"
                    android:layout_height="@dimen/_85pxh"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_38pxh"/>
                <FrameLayout android:id="@+id/frameStackunion"
                    android:layout_height="@dimen/_115pxv" android:layout_width="match_parent">
                    <ImageView android:id="@+id/imageUnion"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_115pxh"
                        android:layout_width="@dimen/_375pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_union}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_union"/>
                    <LinearLayout android:id="@+id/linearRow"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout android:id="@+id/linearRowhome"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_19pxv"
                            android:layout_weight="0.72"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <LinearLayout
                                android:id="@+id/linearColumnhome"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_43pxv"
                                android:layout_weight="1"
                                android:layout_width="0dp" android:orientation="vertical">
                                <ImageView android:id="@+id/imageHome"
                                    android:layout_gravity="center"
                                    android:layout_height="@dimen/_23pxh"
                                    android:layout_width="@dimen/_25pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_home"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>
                                <TextView android:ellipsize="end"
                                    android:gravity="center_horizontal"
                                    android:id="@+id/txtHOMEOne"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_2pxh"
                                    android:layout_marginStart="@dimen/_2pxh"
                                    android:layout_marginTop="@dimen/_9pxv"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{detailArticlesVM.detailArticlesModel.txtHOMEOne}"
                                    style="@style/txtAndadaProromanbold10_1" tools:text="@string/lbl_home"/>
                            </LinearLayout>
                            <FrameLayout
                                android:id="@+id/frameStackplusOne"
                                android:layout_height="@dimen/_58pxv"
                                android:layout_marginBottom="@dimen/_30pxv"
                                android:layout_width="@dimen/_58pxh" style="@style/groupStyleblue_400cornerRadius">
                                <ImageView
                                    android:id="@+id/imagePlusOne"
                                    android:layout_gravity="center"
                                    android:layout_height="@dimen/_21pxh"
                                    android:layout_marginBottom="@dimen/_17pxv"
                                    android:layout_marginTop="@dimen/_18pxv"
                                    android:layout_width="@dimen/_21pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_plus"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_plus"/>
                            </FrameLayout>
                        </LinearLayout>
                        <LinearLayout android:id="@+id/linearRowuser"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_23pxv"
                            android:layout_weight="0.28"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <LinearLayout
                                android:id="@+id/linearColumnuser"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_19pxv"
                                android:layout_marginTop="@dimen/_20pxv"
                                android:layout_weight="1"
                                android:layout_width="0dp" android:orientation="vertical">
                                <ImageView android:id="@+id/imageUser"
                                    android:layout_gravity="center"
                                    android:layout_height="@dimen/_25pxh"
                                    android:layout_width="@dimen/_19pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_user_bluegray_100"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_user_bluegray_100"/>
                                <TextView android:ellipsize="end"
                                    android:gravity="center_horizontal"
                                    android:id="@+id/txtPROFILE"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_2pxh"
                                    android:layout_marginStart="@dimen/_2pxh"
                                    android:layout_marginTop="@dimen/_8pxv"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{detailArticlesVM.detailArticlesModel.txtPROFILE}"
                                    style="@style/txtAndadaProromanbold10_2" tools:text="@string/lbl_profile"/>
                            </LinearLayout>
                            <ImageView
                                android:id="@+id/imageRectangleThirtyOne"
                                android:layout_gravity="center"
                                android:layout_height="@dimen/_85pxh"
                                android:layout_marginEnd="@dimen/_1pxh"
                                android:layout_width="@dimen/_38pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_rectangle31_4"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle31_4"/>
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>
            </FrameLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
