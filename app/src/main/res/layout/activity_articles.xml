<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="articlesVM" type="com.phngsapplication.app.modules.articles.data.viewmodel.ArticlesVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnellipse"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearArticles"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameStackarrowleft"
                android:layout_height="@dimen/_174pxv" android:layout_width="match_parent">
                <LinearLayout android:id="@+id/linearRowarrowleft"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStyle">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnarrowleft"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_42pxv"
                        android:layout_marginStart="@dimen/_23pxh"
                        android:layout_marginTop="@dimen/_31pxv"
                        android:layout_weight="0.52"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageArrowleft"
                            android:layout_height="@dimen/_19pxh"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="@dimen/_11pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowleft"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtArticles"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_19pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{articlesVM.articlesModel.txtArticles}"
                            style="@style/txtAndadaProromanbold30" tools:text="@string/lbl_articles"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageEllipse"
                        android:layout_height="@dimen/_86pxh"
                        android:layout_marginBottom="@dimen/_63pxv"
                        android:layout_width="@dimen/_168pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_ellipse}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageEllipseOne"
                    android:layout_gravity="bottom|end"
                    android:layout_height="@dimen/_124pxh"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="@dimen/_75pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_ellipse_124x75}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse_124x75"/>
                <View
                    android:background="@drawable/rectangle_bg_white_a702_radius_25"
                    android:id="@+id/viewRectangle"
                    android:layout_gravity="bottom|center"
                    android:layout_height="@dimen/_50pxv"
                    android:layout_marginEnd="@dimen/_22pxh"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_331pxh"/>
                <TextView android:ellipsize="end"
                    android:gravity="top|start"
                    android:id="@+id/txtArticlesOne"
                    android:layout_gravity="bottom|end"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_21pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_21pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{articlesVM.articlesModel.txtArticlesOne}"
                    style="@style/txtSFProTextheavy67" tools:text="@string/lbl_articles"/>
                <LinearLayout android:id="@+id/linearRowsearch"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginEnd="@dimen/_47pxh"
                    android:layout_marginStart="@dimen/_47pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageSearch"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_13pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_search"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_search"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSearchForArti"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_weight="0.9"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{articlesVM.articlesModel.txtSearchForArti}"
                        style="@style/txtAndadaProromanmedium14" tools:text="@string/msg_search_for_arti"/>
                </LinearLayout>
            </FrameLayout>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerArticles"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_30pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_25pxh}" tools:itemCount="2" tools:listitem="@layout/row_articles"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStyle_1">
            <FrameLayout android:id="@+id/frameStackrectanglethree"
                android:layout_height="@dimen/_115pxv" android:layout_width="match_parent">
                <View android:background="@color/white_A702"
                    android:id="@+id/viewRectangleThree"
                    android:layout_height="@dimen/_85pxh"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_38pxh"/>
                <FrameLayout android:id="@+id/frameStackunion"
                    android:layout_height="@dimen/_115pxv" android:layout_width="match_parent">
                    <ImageView android:id="@+id/imageUnion"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_115pxh"
                        android:layout_width="@dimen/_375pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_union}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_union"/>
                    <LinearLayout android:id="@+id/linearRow"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout android:id="@+id/linearRowhome"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_18pxv"
                            android:layout_weight="0.72"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <LinearLayout
                                android:id="@+id/linearColumnhome"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_43pxv"
                                android:layout_weight="1"
                                android:layout_width="0dp" android:orientation="vertical">
                                <ImageView android:id="@+id/imageHome"
                                    android:layout_gravity="center"
                                    android:layout_height="@dimen/_23pxh"
                                    android:layout_width="@dimen/_25pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_home"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>
                                <TextView android:ellipsize="end"
                                    android:gravity="center_horizontal"
                                    android:id="@+id/txtHOMEOne"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_2pxh"
                                    android:layout_marginStart="@dimen/_2pxh"
                                    android:layout_marginTop="@dimen/_10pxv"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{articlesVM.articlesModel.txtHOMEOne}"
                                    style="@style/txtSFProTextmedium10" tools:text="@string/lbl_home"/>
                            </LinearLayout>
                            <FrameLayout
                                android:id="@+id/frameStackplus"
                                android:layout_height="@dimen/_58pxv"
                                android:layout_marginBottom="@dimen/_31pxv"
                                android:layout_width="@dimen/_58pxh" style="@style/groupStyleblue_400cornerRadius">
                                <ImageView android:id="@+id/imagePlus"
                                    android:layout_gravity="center"
                                    android:layout_height="@dimen/_21pxh"
                                    android:layout_marginBottom="@dimen/_17pxv"
                                    android:layout_marginTop="@dimen/_18pxv"
                                    android:layout_width="@dimen/_21pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_plus"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_plus"/>
                            </FrameLayout>
                        </LinearLayout>
                        <LinearLayout android:id="@+id/linearRowuserTwo"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_23pxv"
                            android:layout_weight="0.28"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <LinearLayout
                                android:id="@+id/linearColumnuserTwo"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_18pxv"
                                android:layout_marginTop="@dimen/_20pxv"
                                android:layout_weight="1"
                                android:layout_width="0dp" android:orientation="vertical">
                                <ImageView
                                    android:id="@+id/imageUserTwo"
                                    android:layout_gravity="center"
                                    android:layout_height="@dimen/_25pxh"
                                    android:layout_width="@dimen/_19pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_user_bluegray_100"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_user_bluegray_100"/>
                                <TextView android:ellipsize="end"
                                    android:gravity="center_horizontal"
                                    android:id="@+id/txtPROFILE"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_2pxh"
                                    android:layout_marginStart="@dimen/_2pxh"
                                    android:layout_marginTop="@dimen/_9pxv"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{articlesVM.articlesModel.txtPROFILE}"
                                    style="@style/txtSFProTextmedium10_1" tools:text="@string/lbl_profile"/>
                            </LinearLayout>
                            <ImageView
                                android:id="@+id/imageRectangleThirtyOne"
                                android:layout_gravity="center"
                                android:layout_height="@dimen/_85pxh"
                                android:layout_marginEnd="@dimen/_1pxh"
                                android:layout_marginStart="@dimen/_2pxh"
                                android:layout_width="@dimen/_38pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_rectangle31"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle31"/>
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>
            </FrameLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
