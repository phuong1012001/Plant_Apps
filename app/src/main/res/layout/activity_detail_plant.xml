<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="detailPlantVM" type="com.phngsapplication.app.modules.detailplant.data.viewmodel.DetailPlantVM"/>
    </data>
    <FrameLayout android:id="@+id/frameDetailPlant"
