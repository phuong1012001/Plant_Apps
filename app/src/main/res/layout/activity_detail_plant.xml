                <View android:background="@color/white_A702"
                    android:id="@+id/viewRectangleThree"
                    android:layout_height="@dimen/_85pxh"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_38pxh"/>
